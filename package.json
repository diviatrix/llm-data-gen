{
  "name": "@1337plus/llmdatagen",
  "version": "1.1.3",
  "description": "Universal JSON data generator using LLM with configurable schemas",
  "main": "index.js",
  "bin": {
    "llmdatagen": "index.js"
  },
  "scripts": {
    "start": "node index.js",
    "start:env": "node --env-file=.env index.js",
    "web": "node server.js",
    "web:env": "node --env-file=.env server.js",
    "web:local": "node -e \"process.env.LOCAL_NETWORK_MODE='true'; require('./server.js')\"",
    "dev:web": "node server.js",
    "lint": "eslint .",
    "test": "vitest run",
    "test:unit": "vitest run test/unit",
    "test:integration": "vitest run test/integration",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest",
    "postinstall": "node scripts/postinstall.js",
    "build-css": "postcss public/css/main.css -o public/css/app.css",
    "watch-css": "postcss public/css/main.css -o public/css/app.css --watch"
  },
  "keywords": [
    "llm",
    "data-generation",
    "json-schema",
    "openrouter",
    "cli"
  ],
  "author": "1337.plus",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/diviatrix/llm-data-gen.git"
  },
  "homepage": "https://github.com/diviatrix/llm-data-gen#readme",
  "bugs": {
    "url": "https://github.com/diviatrix/llm-data-gen/issues"
  },
  "dependencies": {
    "ajv": "^8.12.0",
    "ajv-formats": "^3.0.1",
    "bcrypt": "^5.1.1",
    "better-sqlite3": "^12.2.0",
    "commander": "^11.1.0",
    "cors": "^2.8.5",
    "dompurify": "^3.2.6",
    "dotenv": "^17.2.1",
    "express": "^4.18.2",
    "inquirer": "^9.3.7",
    "inquirer-autocomplete-prompt": "^3.0.1",
    "jsonwebtoken": "^9.0.2",
    "marked": "^16.1.2",
    "multer": "^2.0.0",
    "sqlite3": "^5.1.6"
  },
  "devDependencies": {
    "@unocss/cli": "^66.3.3",
    "@unocss/postcss": "^66.3.3",
    "@vitest/coverage-v8": "^1.6.1",
    "@vitest/ui": "^1.6.1",
    "eslint": "^8.54.0",
    "postcss": "^8.5.6",
    "postcss-cli": "^11.0.1",
    "postcss-import": "^16.1.1",
    "unocss": "^66.3.3",
    "vitest": "^1.6.1"
  },
  "type": "module",
  "engines": {
    "node": ">=18.0.0"
  }
}
